<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <generic-templates j:dependencies="bootstrap bootstrap-components default layout-generic-components"
                         j:installedModules="generic-templates"
                         j:moduleType="templatesSet"
                         j:title="generic-templates"
                         jcr:primaryType="jnt:module">
                
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder"/>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/generic-templates/templates/files/themes/default"
                  j:view="generic-templates"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                    <home jcr:primaryType="jnt:pageTemplate"/>
                    <simple jcr:primaryType="jnt:pageTemplate"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <header jcr:primaryType="jlayout:section" sectionElement="header">
                                <header jcr:primaryType="jnt:contentList">
                                    <header-top fluid="true"
                                             j:classname="header-top-content"
                                             j:id="header-top"
                                             j:skin="skins.custom"
                                             jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                             jcr:primaryType="jnt:bootstrapContainer">
                                        <header-top jcr:primaryType="jnt:contentList">
                                            <header-top-row fluid="true" jcr:primaryType="jnt:bootstrapRow">
                                                <header-top-row-column jcr:primaryType="jnt:bootstrapColumn" offset="0" span="12"/>
                                            </header-top-row>
                                    </header-top>
                                    </header-top>
                                    <nav fluid="true"
                                             j:classname="navbar"
                                             j:id="nav"
                                             j:skin="skins.custom"
                                             jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                             jcr:primaryType="jnt:bootstrapContainer">
                                        <nav jcr:primaryType="jnt:contentList">
                                            <nav-container fluid="true" jcr:primaryType="jnt:bootstrapContainer">
                                                <nav-container jcr:primaryType="jnt:contentList">
                                                    <nav-row fluid="true" jcr:primaryType="jnt:bootstrapRow">
                                                        <nav-column jcr:primaryType="jnt:bootstrapColumn" offset="0" span="12"/>
                                                    </nav-row>
                                            </nav-container>
                                            </nav-container>
                                    </nav>
                                    </nav>
                            </header>
                            </header>
                            <pagecontent j:allowedTypes="" jcr:primaryType="jnt:area"/>
                            <footer jcr:primaryType="jlayout:section" sectionElement="footer">
                                <footer jcr:primaryType="jnt:contentList">
                                    <action cssClassName="section-call-for-action"
                                jcr:primaryType="jlayout:section"
                                sectionElement="section">
                                        <action jcr:primaryType="jnt:contentList">
                                    <action-container fluid="true" jcr:primaryType="jnt:bootstrapContainer">
                                        <action-container jcr:primaryType="jnt:contentList">
                                            <action-row fluid="true" jcr:primaryType="jnt:bootstrapRow">
                                                        <action-column-left jcr:primaryType="jnt:bootstrapColumn" offset="0" span="4"/>
                                                        <action-column-center jcr:primaryType="jnt:bootstrapColumn" offset="0" span="4"/>
                                                        <action-column-right jcr:primaryType="jnt:bootstrapColumn" offset="0" span="4"/>
                                            </action-row>
                                    </action-container>
                                    </action-container>
                                    </action>
                                    </action>
                                    <copyright cssClassName="copyright"
                                   jcr:primaryType="jlayout:section"
                                   sectionElement="section">
                                        <copyright jcr:primaryType="jnt:contentList">
                                            <copyright-container fluid="true" jcr:primaryType="jnt:bootstrapContainer">
                                                <copyright-container jcr:primaryType="jnt:contentList">
                                                    <copyright-row fluid="true" jcr:primaryType="jnt:bootstrapRow">
                                                        <copyright-column-left jcr:primaryType="jnt:bootstrapColumn" offset="0" span="6"/>
                                                        <copyright-column-right jcr:primaryType="jnt:bootstrapColumn" offset="0" span="6"/>
                                                    </copyright-row>
                                            </copyright-container>
                                            </copyright-container>
                                    </copyright>
                                    </copyright>
                            </footer>
                            </footer>
                        </pagecontent>
                </base>
                    <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="generic-templates"
                              jcr:primaryType="jnt:contentTemplate">
                    <pagecontent jcr:primaryType="jnt:contentList">
                            <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                    </pagecontent>
                </content-template>
            </templates>
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
        </generic-templates>
    </modules>
</content>
